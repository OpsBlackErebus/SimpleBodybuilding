    "express": "^4.16.3",
    "jsonwebtoken": "^8.5.0",
    "mongoose": "^5.1.6",
    "react-calendar": "^2.19.0"
  },
  "devDependencies": {
    "nodemon": "^1.17.5"
  }
}
